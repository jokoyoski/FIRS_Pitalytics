
@model Pitalytics.Interfaces.ITaxReportView

@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <div class="container">
        <a class="btn btn default-form" href="TaxReport">Go Back to Tax Report</a>
    </div>
</div>

<div class="container-fluid">
    @if (Model.FileTypeId > 0)
    {
    <a class="btn btn-primary mt-2" href="/Administration/FileResult?fileId=@Model.FileTypeId">Download File Format</a>

    }


    @using (Html.BeginForm("AddTaxReport", "AgentOfDeduction", FormMethod.Post, new { id = "taxReportForm", enctype = "multipart/form-data", encoding = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        <div class="container">


            <div class="row no-gutters pt-5">
                <div style="text-align:center" class="card col-md-5 m-auto">


                    <span style="color:green">
                        @Model.ProcessingMessage
                    </span>
                    <span style="color:red">
                        @Html.ValidationSummary()
                    </span>

                    <div class="card-body">
                        <p class="text-danger"></p>
                        <p class="text-danger"></p>
                        <div class="container-fluid">



                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="form-group ic-cmp-int float-lb floating-lb">
                                    <div class="form-ic-cmp">
                                        <label>Upload File</label>
                                        <input data-placement="left" class="file" title="Upload Level Excel Sheet" type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" name="taxReportExcelSheet" value="" />
                                    </div>
                                </div>
                            </div>


                            <div class="form-group ">
                                <label class="control-label"> Select    IncomeType <span class="text-danger">*</span></label>


                                @Html.DropDownList("IncomeTypeId", Model.IncomeTypes, new { @class = "TimingId form-control" })
                            </div>


                            <div class="form-group">

                                <div class="">
                                    <input type="text" placeholder="YEAR" name="Year" required id="Company" value="@Model.Year" class="form-control text-type" />
                                </div>
                            </div>

                            <div style="text-align:center">
                                <input type="submit" id="submit" name="login" value="Submit" style="color:white;" class="btn btn-primary mt-2" />
                            </div>

                        </div>



                    </div>


                </div>
            </div>
        </div>
    }

</div>





